<div class="row">
  <div class="small-12 columns">
    <h1>Edit Image</h1>

    <%= render :partial => 'shared/errors', :locals => { :item => @image } %>

    <%= form_for :image, url: campaign_image_path(@campaign, @image), method: :patch do |f| %>

      <%= render :partial => "form", :locals => {:f => f} %>

      <div>
        <%= f.submit %>
      </div>
    <% end %>
  </div>
</div>
