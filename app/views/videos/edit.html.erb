<div class="row">
  <div class="small-12 columns">
    <h1>Edit Video</h1>
    <%= form_for :video, url: campaign_videos_path(@campaign), method: :patch do |f| %>

      <%= render :partial => "form", :locals => {:f => f} %>

      <div>
        <%= f.submit %>
      </div>
    <% end %>
  </div>
</div>
